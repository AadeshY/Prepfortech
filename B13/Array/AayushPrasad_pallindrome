 bool isPalindrome(int x) {
       
       long sum=0;
        int ac = x;
        
        while(x>0){
            int rem=0;
            rem = x%10;
            sum = sum*10 + rem;
            x /= 10;
        }
        
        if(sum == ac)
            return true;
        else 
            return false;
    }
